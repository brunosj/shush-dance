{
  "name": "shush",
  "description": "shush dance",
  "version": "2.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NEXT_BUILD=true node dist/server.js",
    "build:next-green": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NEXT_BUILD=true PORT=3001 node dist/server.js",
    "build": "cross-env NODE_ENV=production npm run build:payload && npm run build:server && npm run build:next",
    "build-green": "cross-env NODE_ENV=production PORT=3001 npm run build:payload && npm run build:server && npm run build:next",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NODE_ENV=production PORT=3000 node dist/server.js",
    "serve-green": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NODE_ENV=production PORT=3001 node dist/server.js",
    "eject": "yarn remove next react react-dom @next/eslint-plugin-next && ts-node eject.ts",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,ttf,woff,woff2,eot,svg,jpg,png,js}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:graphQLSchema",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload"
  },
  "dependencies": {
    "@payloadcms/bundler-webpack": "^1.0.7",
    "@payloadcms/db-mongodb": "^1.7.5",
    "@payloadcms/email-nodemailer": "^3.56.0",
    "@payloadcms/live-preview-react": "^0.2.0",
    "@payloadcms/plugin-nested-docs": "^1.0.12",
    "@payloadcms/plugin-redirects": "^1.0.2",
    "@payloadcms/plugin-seo": "^2.4.0",
    "@payloadcms/richtext-slate": "^1.5.2",
    "@paypal/react-paypal-js": "^8.9.1",
    "@stripe/react-stripe-js": "^3.10.0",
    "@stripe/stripe-js": "^4.10.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "escape-html": "^1.0.3",
    "express": "^4.21.2",
    "fs": "0.0.1-security",
    "hamburger-react": "^2.5.2",
    "lodash": "^4.17.21",
    "music-metadata": "^10.9.1",
    "next": "14.2.15",
    "nodemailer": "^7.0.6",
    "payload": "^2.32.3",
    "qs": "6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-h5-audio-player": "^3.10.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "6.27.0",
    "sharp": "0.33.5",
    "stripe": "^18.5.0",
    "use-shopping-cart": "^3.2.0",
    "yet-another-react-lightbox": "^3.25.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.32",
    "@payloadcms/eslint-config": "^1.1.1",
    "@swc/core": "1.7.36",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^5.0.3",
    "@types/node": "22.7.5",
    "@types/qs": "^6.14.0",
    "@types/react": "18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "file-loader": "^6.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "slate": "0.110.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "10.9.2",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "overrides": {
      "sharp": "0.33.5"
    }
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
